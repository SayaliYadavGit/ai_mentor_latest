{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.sharepoint.com https://*.office.com https://*.microsoft.com"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://sharepoint.com"
        }
      ]
    }
  ]
}